{% extends "base.html.tmpl" %}

{% block page_settings %}
{% set active_menu = 'typeSystem'%}
{% endblock %}


{% block sidebar %}
        <div class="col-sm-3 col-md-2 sidebar">
          <ul class="nav nav-sidebar">
            <li ><a href="#">Documents Sets</a></li>
            <li ><a href="#">Annotation Sets</a></li>
            <li><a href="#">Documents</a></li>
          </ul>
        </div>
{% endblock %}

{% block mainContent %}
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
          <h1 class="page-header">Dictionaries</h1>
          <div class="row placeholders">
            <form action="{{ url_for('typesystem.import_typesystem',project_id=project.id)}}" enctype="multipart/form-data" data-ajax="true" method="POST" class="navbar-form navbar-left" role="search">
              <div class="form-group">
                <input type="text" class="form-control" placeholder="Search">
              </div>
              <button type="submit" class="btn btn-default">Submit</button>

                <div class="input-group">
                  <input type="file" name="file" class="form-control" placeholder="Recipient's username" aria-describedby="basic-addon2">
                  <span class="input-group-addon" id="basic-addon2">@example.com</span>
                </div>


            </form>
            <form action="{{ url_for('project.documents_export',project_id=project.id)}}" enctype="multipart/form-data" data-ajax="true" method="POST" class="navbar-form navbar-left" role="search">
              <button type="submit" class="btn btn-default">Export</button>
            </form>
          </div>





        </div>

{% endblock %}